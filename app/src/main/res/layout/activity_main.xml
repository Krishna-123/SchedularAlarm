<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.krishna.schedular.MainActivity">

    <Button
        android:text="SU"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:id="@+id/button"
        android:background="@drawable/rounded"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp" />

    <Button
        android:text="MO"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="60dp"
        android:layout_marginStart="60dp"
        android:id="@+id/button1"
        android:background="@drawable/rounded"
        android:onClick="monDay"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp" />

    <Button
        android:text="TU"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="110dp"
        android:layout_marginStart="110dp"
        android:id="@+id/button2"
        android:background="@drawable/rounded"
        android:onClick="tueDay"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp" />
    <Button
        android:text="WE"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="160dp"
        android:layout_marginStart="160dp"
        android:id="@+id/button3"
        android:background="@drawable/rounded"
        android:onClick="wedDay"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp" />
    <Button
        android:text="TH"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="210dp"
        android:layout_marginStart="210dp"
        android:id="@+id/button4"
        android:background="@drawable/rounded"
        android:onClick="thuDay"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp" />
    <Button
        android:text="FR"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="260dp"
        android:layout_marginStart="260dp"
        android:id="@+id/button5"
        android:background="@drawable/rounded"
        android:onClick="friDay"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp" />

    <Button
        android:text="SA"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="310dp"
        android:layout_marginStart="310dp"
        android:id="@+id/button6"
        android:background="@drawable/rounded"
        android:onClick="satDay"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp" />

    <Button
        android:text="Start Service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="106dp"
        android:id="@+id/buttonStart"
        android:layout_below="@+id/button"
        android:layout_alignStart="@+id/button"
        android:onClick="startOnclick"/>

    <Button
        android:text="Stop Service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/buttonStop"
        android:layout_alignBaseline="@+id/buttonStart"
        android:layout_alignBottom="@+id/buttonStart"
        android:layout_alignParentEnd="true"
        android:onClick="stopOnClick"/>


</RelativeLayout>
